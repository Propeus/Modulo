<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net7.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<TargetName>Module</TargetName>

		<Product>Propeus.Modulo.Console</Product>
		<VersionPrefix>0.1.0</VersionPrefix>
		<Description>Modulo para controle do gerenciador por linha de comando</Description>

		<Authors>Propeus</Authors>
		<Company>Propeus</Company>

		<RepositoryType>GIT</RepositoryType>
		<RepositoryUrl>https://github.com/Propeus/Modulo</RepositoryUrl>

		<PackageProjectUrl>https://github.com/Propeus/Modulo</PackageProjectUrl>
		<PackageTags>Propeus Net Modulo Console</PackageTags>
		<PackageId>Propeus.Modulo.Console</PackageId>

		<AssemblyName>Module</AssemblyName>
		<RootNamespace>Propeus.Modulo.Console</RootNamespace>

		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<IncludeSymbols>true</IncludeSymbols>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
		<EmbedUntrackedSources>true</EmbedUntrackedSources>
		<PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
	</PropertyGroup>

	<ItemGroup>
		<ProjectReference Include="..\..\core\Propeus.Modulo.Abstrato\Propeus.Module.Abstract.csproj" />
		<ProjectReference Include="..\..\core\Propeus.Modulo.Core\Propeus.Module.Manager.csproj" />
	</ItemGroup>

</Project>
