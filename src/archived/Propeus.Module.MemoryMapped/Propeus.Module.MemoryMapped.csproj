<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	  <TargetFrameworks>net7.0</TargetFrameworks>
	  <ImplicitUsings>enable</ImplicitUsings>
	  <Nullable>enable</Nullable>

	  <Product>Propeus.Modulo.MemoryMapped</Product>
	  <VersionPrefix>0.1.0</VersionPrefix>
	  <Description></Description>

	  <Authors>Propeus</Authors>
	  <Company>Propeus</Company>

	  <RepositoryType>GIT</RepositoryType>
	  <RepositoryUrl>https://github.com/Propeus/Modulo</RepositoryUrl>

	  <PackageProjectUrl>https://github.com/Propeus/Modulo</PackageProjectUrl>
	  <PackageTags>Propeus Net Modulo</PackageTags>
	  <PackageId>Propeus.Modulo.MemoryMapped</PackageId>

	  <AssemblyName>Propeus.Modulo.MemoryMapped</AssemblyName>
	  <RootNamespace>Propeus.Modulo.MemoryMapped</RootNamespace>

	  <GenerateDocumentationFile>true</GenerateDocumentationFile>
	  <IncludeSymbols>true</IncludeSymbols>
	  <SymbolPackageFormat>snupkg</SymbolPackageFormat>
	  <EmbedUntrackedSources>true</EmbedUntrackedSources>
	  <PackageLicenseFile>LICENSE</PackageLicenseFile>
	  <PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\core\Propeus.Modulo.Abstrato\Propeus.Module.Abstract.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Contracts\" />
    <Folder Include="Models\" />
  </ItemGroup>

</Project>
