<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net7.0</TargetFrameworks>
		<LangVersion>latest</LangVersion>
		<Nullable>enable</Nullable>

		<Product>Propeus.Module.Watcher</Product>
		<VersionPrefix>0.1.0</VersionPrefix>
		<Description>Modulo para mapear outros modulos em disco</Description>

		<Authors>Propeus</Authors>
		<Company>Propeus</Company>

		<RepositoryType>GIT</RepositoryType>
		<RepositoryUrl>https://github.com/Propeus/Modulo</RepositoryUrl>

		<PackageProjectUrl>https://github.com/Propeus/Modulo</PackageProjectUrl>
		<PackageTags>Propeus Net Modulo Watcher</PackageTags>
		<PackageId>Propeus.Module.Watcher</PackageId>
		<PackageLicenseFile>LICENCE</PackageLicenseFile>
		<PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>

		<AssemblyName>Propeus.Module.Watcher</AssemblyName>
		<RootNamespace>Propeus.Module.Watcher</RootNamespace>

		<GeneratePackageOnBuild>false</GeneratePackageOnBuild>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>

		<IncludeSymbols>true</IncludeSymbols>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
		<EmbedUntrackedSources>true</EmbedUntrackedSources>
	</PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\..\core\Propeus.Modulo.Abstrato\Propeus.Modulo.Abstrato.csproj" />
    <ProjectReference Include="..\..\core\Propeus.Modulo.IL.Core\Propeus.Modulo.IL.Core.csproj" />
  </ItemGroup>

</Project>
