<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net7.0</TargetFrameworks>

		<Product>Propeus.Modulo.Util</Product>
		<VersionPrefix>0.1.0</VersionPrefix>
		<Description>Componente com classes para facilitar o desenvolvimento de modulos</Description>

		<Authors>Propeus</Authors>
		<Company>Propeus</Company>

		<RepositoryType>GIT</RepositoryType>
		<RepositoryUrl>https://github.com/Propeus/Modulo</RepositoryUrl>

		<PackageProjectUrl>https://github.com/Propeus/Modulo</PackageProjectUrl>
		<PackageTags>Propeus Net Modulo Util</PackageTags>
		<PackageId>Propeus.Modulo.Util</PackageId>

		<AssemblyName>Propeus.Modulo.Util</AssemblyName>
		<RootNamespace>Propeus.Modulo.Util</RootNamespace>

		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<IncludeSymbols>true</IncludeSymbols>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
		<EmbedUntrackedSources>true</EmbedUntrackedSources>
		<PackageLicenseFile>Propeus.Modulo.Util.csproj</PackageLicenseFile>
		<PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
	</PropertyGroup>

</Project>
