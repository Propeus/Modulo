<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net7.0</TargetFrameworks>
		<Nullable>enable</Nullable>
		
		<Product>Propeus.Modulo.Abstrato</Product>
		<VersionPrefix>0.1.0</VersionPrefix>
		<Description>Componente com classes e interfaces para implementacao de modulos</Description>

		<Authors>Propeus</Authors>
		<Company>Propeus</Company>

		<RepositoryType>GIT</RepositoryType>
		<RepositoryUrl>https://github.com/Propeus/Modulo</RepositoryUrl>

		<PackageProjectUrl>https://github.com/Propeus/Modulo</PackageProjectUrl>
		<PackageTags>Propeus Net Modulo Abstrato</PackageTags>
		<PackageId>Propeus.Modulo.Abstrato</PackageId>

		<AssemblyName>Propeus.Modulo.Abstrato</AssemblyName>
		<RootNamespace>Propeus.Modulo.Abstrato</RootNamespace>

		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<IncludeSymbols>true</IncludeSymbols>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
		<EmbedUntrackedSources>true</EmbedUntrackedSources>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
		<PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
	</PropertyGroup>

	<ItemGroup>
	  <ProjectReference Include="..\Propeus.Modulo.Util\Propeus.Module.Utils.csproj" />
	</ItemGroup>

	<ItemGroup>
	  <None Update="LICENSE">
	    <Pack>True</Pack>
	    <PackagePath>\</PackagePath>
	  </None>
	</ItemGroup>

</Project>
