<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net7.0</TargetFrameworks>
		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
	</PropertyGroup>
	<PropertyGroup>
		<Product>Propeus.Module.Manager.Dynamic.Package</Product>
		<AssemblyName>Propeus.Module.Manager.Dynamic.Package</AssemblyName>
		<RootNamespace>Propeus.Module.Manager.Dynamic.Package</RootNamespace>
		<GenerateDocumentationFile>true</GenerateDocumentationFile>
		<IncludeSymbols>true</IncludeSymbols>
	</PropertyGroup>
	<PropertyGroup>
		<VersionPrefix>0.1.0</VersionPrefix>
		<Description>Componente gerenciar modulos em tempo de execução por mieo de pacotes</Description>
		<Authors>Propeus</Authors>
		<Company>Propeus</Company>
	</PropertyGroup>
	<PropertyGroup>
		<RepositoryType>GIT</RepositoryType>
		<RepositoryUrl>https://github.com/Propeus/Modulo</RepositoryUrl>
	</PropertyGroup>
	<PropertyGroup>
		<PackageProjectUrl>https://github.com/Propeus/Modulo</PackageProjectUrl>
		<PackageTags>Propeus Net Modulo Gerenciador Dinamico Pacote</PackageTags>
		<PackageId>Propeus.Module.Manager.Dynamic.Package</PackageId>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
		<PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
		<EmbedUntrackedSources>true</EmbedUntrackedSources>
	</PropertyGroup>

  <ItemGroup>
    <Folder Include="Models\" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Propeus.Module.Abstract\Propeus.Module.Abstract.csproj" />
  </ItemGroup>

</Project>
