<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	  <TargetFrameworks>net7.0</TargetFrameworks>

	  <Product>Propeus.Modulo.WorkerService</Product>
	  <VersionPrefix>0.1.0</VersionPrefix>
	  <Description>Modulo para inicializar como workerservice</Description>

	  <Authors>Propeus</Authors>
	  <Company>Propeus</Company>

	  <RepositoryType>GIT</RepositoryType>
	  <RepositoryUrl>https://github.com/Propeus/Modulo</RepositoryUrl>

	  <PackageProjectUrl>https://github.com/Propeus/Modulo</PackageProjectUrl>
	  <PackageLicenseExpression>MIT</PackageLicenseExpression>
	  <PackageTags>Propeus Net Modulo WorkerService</PackageTags>
	  <PackageId>Propeus.Modulo.WorkerService</PackageId>

	  <AssemblyName>Propeus.Modulo.WorkerService</AssemblyName>
	  <RootNamespace>Propeus.Modulo.WorkerService</RootNamespace>

	  <GenerateDocumentationFile>true</GenerateDocumentationFile>
	  <IncludeSymbols>true</IncludeSymbols>
	  <SymbolPackageFormat>snupkg</SymbolPackageFormat>
	  <EmbedUntrackedSources>true</EmbedUntrackedSources>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Hosting.Abstractions" Version="7.0.0" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Propeus.Modulo.Abstrato\Propeus.Modulo.Abstrato.csproj" />
  </ItemGroup>

</Project>
